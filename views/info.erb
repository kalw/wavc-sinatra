<script>
$(document).ready(function(){   
$("#airplay").click(function(){   
page=($(this).attr("href")); 
$.ajax({ 
url: page, 
cache: false, 
success:function(html){ 
afficher(html);     
},
error:function(XMLHttpRequest, textStatus, errorThrows){ 
}
});
return false; 
});
});function afficher(donnees){ 
$("#contenu").empty(); 
$("#contenu").append(donnees); 
}
</script>
